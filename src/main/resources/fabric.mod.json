{
	"schemaVersion": 1,
	"id": "iconitems",
	"version": "${version}",
	"name": "Icon Items",
	"description": "This mod adds various icons from various places as items in-game.",
	"authors": [
		"Blurryface"
	],
	"contact": {
		"homepage": "https://blurryface.xyz/",
		"sources": "https://github.com/blryface/icon-items"
	},
	"license": "CC BY-NC-SA 4.0",
	"icon": "assets/iconitems/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"xyz.blurryface.iconitems.Iconitems"
		],
		"client": [
			"xyz.blurryface.iconitems.IconitemsClient"
		]
	},
	"mixins": [
		"iconitems.mixins.json",
		{
			"config": "iconitems.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": "~1.20.2",
		"java": ">=17",
